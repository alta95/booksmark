<%= form_with model: @bookmark do |form| %>
  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %><br>
  </div>

  <div>
    <%= form.label :url %><br>
    <%= form.text_field :url %><br>
  </div>

  <div>
    <%= form.label :title %><br>
    <%= form.number_field :rating %><br>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>